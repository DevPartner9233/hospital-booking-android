<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aman.hospitalappointy">



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Patient_DoctorProfileActivity" />
        <activity android:name=".Patient_RegistrationActivity"
            android:parentActivityName=".LoginActivity"/>
        <activity android:name=".Patient_BookAppointmentActivity" />
        <activity android:name=".Patient_ShowBookedAppointmentActivity" />
        <activity android:name=".Doctor_ProfileActivity" />
        <activity android:name=".Doctor_EditProfileActivity" />
        <activity android:name=".Doctor_RosterPlanActivity" />
        <activity android:name=".Doctor_EditRosterPlanActivity" />
        <activity android:name=".Doctor_ShowAppointmentActivity" />
        <activity android:name=".SplashScreenActivity"></activity>
    </application>

</manifest>